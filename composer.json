{
    "name":"contao-bootstrap/form",
    "description":"Contao form templates for Bootstrap 4",
    "keywords":["bootstrap", "form", "contao"],
    "type":"contao-bundle",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"David Molineus",
            "email":"mail@netzmacht.de",
            "homepage":"https://netzmacht.de",
            "role":"Developer"
        },
        {
            "name": "Alexander Kehr",
            "email": "info@kehr-solutions.de",
            "homepage": "https://www.kehr-solutions.de",
            "role": "Developer"
        }
    ],
    "support":{
        "email":"mail@netzmacht.de",
        "issues":"https://github.com/contao-bootstrap/form/issues",
        "wiki":"http://contao-bootstrap.netzmacht.de",
        "source":"https://github.com/contao-bootstrap/form"
    },
    "conflict": {
        "netzmacht/contao-bootstrap": "*"
    },
    "require":{
        "php":">=7.4",
        "contao/core-bundle":"~4.4",
        "contao-bootstrap/core": "~2.0",
        "contao-community-alliance/meta-palettes":"~2.0 || ~1.11",
        "netzmacht/contao-form-designer": "^1.1.7",
        "netzmacht/html": "^2.0",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/http-kernel": "^5.4"
    },
    "require-dev": {
        "phpcq/all-tasks": "^1.2",
        "contao/manager-plugin": "^2.1"
    },
    "autoload": {
        "psr-4": { "ContaoBootstrap\\Form\\": "src/" }
    },
    "extra":{
        "contao-manager-plugin": "ContaoBootstrap\\Form\\ContaoManager\\Plugin",
        "contao": {
            "transifex": {
                "project": "contao-bootstrap",
                "prefix": "form-",
                "languages_cto": "src/Resources/contao/languages",
                "languages_tx": ".tx"
            }
        },
        "branch-alias": {
            "dev-master": "2.1.x-dev",
            "dev-develop": "2.2.x-dev",
            "dev-support/1.x": "1.1.x-dev"
        }
    }
}
